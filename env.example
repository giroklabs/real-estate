# Fly.io 배포를 위한 환경 변수
FLASK_APP=app.py
FLASK_ENV=production
PORT=5001

# 데이터베이스 설정
DATABASE_URL=sqlite:///realstate.db

# API 키들 (실제 값으로 교체 필요)
MOBILE_API_KEY=your_mobile_api_key_here
MOBILE_API_API_SECRET=your_mobile_api_secret_here
REB_API_KEY=your_reb_api_key_here

# 로깅 설정
LOG_LEVEL=INFO
LOG_FILE=logs/app.log

# CORS 설정
CORS_ORIGINS=*
